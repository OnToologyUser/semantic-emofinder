    <!-- https://www.usc.gal/pcc/app/emofinder/Arousal -->

    <owl:Class rdf:about="https://www.usc.gal/pcc/app/emofinder/Arousal">
        <rdfs:subClassOf rdf:resource="https://www.usc.gal/pcc/app/emofinder/EmotionalDimension"/>
        <rdfs:comment xml:lang="en">Arousal refers to the level of calmness or excitement evoked by a word, that is, how relaxing or intense it feels to the person.</rdfs:comment>
        <rdfs:comment xml:lang="es">La activación hace referencia al nivel de calma o excitación que evoca una palabra, es decir, cuán relajante o intensa le resulta a la persona.</rdfs:comment>
        <rdfs:label xml:lang="es">Activación</rdfs:label>
        <rdfs:label xml:lang="en">Arousal</rdfs:label>
    </owl:Class>
    


    <!-- https://www.usc.gal/pcc/app/emofinder/Concreteness -->

    <owl:Class rdf:about="https://www.usc.gal/pcc/app/emofinder/Concreteness">
        <rdfs:subClassOf rdf:resource="https://www.usc.gal/pcc/app/emofinder/SubjectiveDimension"/>
        <rdfs:comment xml:lang="en">Concreteness refers to the extent to which a word refers to an entity that can be perceived with the senses.</rdfs:comment>
        <rdfs:comment xml:lang="es">La concreción se refiere a hasta qué punto la palabra se refiere a una entidad que puede percibirse con los sentidos.</rdfs:comment>
        <rdfs:label xml:lang="es">Concreción</rdfs:label>
        <rdfs:label xml:lang="en">Concreteness</rdfs:label>
    </owl:Class>
    


    <!-- https://www.usc.gal/pcc/app/emofinder/ContextualAvailability -->

    <owl:Class rdf:about="https://www.usc.gal/pcc/app/emofinder/ContextualAvailability">
        <rdfs:subClassOf rdf:resource="https://www.usc.gal/pcc/app/emofinder/SubjectiveDimension"/>
        <rdfs:comment xml:lang="en">Contextual availability refers to the ease with which we can think of a context to which the word could be associated.</rdfs:comment>
        <rdfs:comment xml:lang="es">La disponibilidad contextual se refiere a la facilidad con la que podemos pensar en un contexto al cual se podría asociar la palabra.</rdfs:comment>
        <rdfs:label xml:lang="en">Contextual Availability</rdfs:label>
        <rdfs:label xml:lang="es">Disponibilidad contextual</rdfs:label>
    </owl:Class>
    


