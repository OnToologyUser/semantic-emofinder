    <owl:Class rdf:about="https://www.usc.gal/pcc/app/emofinder/Anger">
        <rdfs:subClassOf rdf:resource="https://www.usc.gal/pcc/app/emofinder/DiscreteEmotionalCategory"/>
        <rdfs:comment xml:lang="en">It refers to the degree of anger conveyed by a word.</rdfs:comment>
